package eng2020_1_a01;

public class Losango extends Poligono {

	public Losango(double base, double altura) {
		super(base, altura);
	}

	@Override
	public double area() {
		return super.area();
	}

	@Override
	public String toString() {
		return "Losango [getBase()=" + getBase() + ", getAltura()=" + getAltura() + "]";
	}

}
